{
  "name": "lm-ui-element-plus",
  "files": [
    "lib"
  ],
  "main": "./lib/lm-ui-element-plus.umd.js",
  "module": "./lib/lm-ui-element-plus.es.js",
  "version": "0.0.1",
  "private": false,
  "author": "杨涟",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "build:lib": "npm run build && npm run build:style && npm run build:utils",
