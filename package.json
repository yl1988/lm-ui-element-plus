    "@vitejs/plugin-vue": "^1.2.5",
    "@vue/compiler-sfc": "^3.0.5",
    "cp-cli": "^1.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass": "^1.35.2",
    "vite": "^2.4.3"
  },
  "exports": {
    ".": {
      "import": "./lib/lm-ui-element-plus.es.js",
      "require": "./lib/lm-ui-element-plus.umd.js"
